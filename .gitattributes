# Adobe Illustrator
*.ai filter=lfs diff=lfs merge=lfs -text       # Illustrator project files
*.aia filter=lfs diff=lfs merge=lfs -text     # Illustrator action files
*.ait filter=lfs diff=lfs merge=lfs -text     # Illustrator template files

# Linked Image Files
*.png filter=lfs diff=lfs merge=lfs -text     # PNG images
*.jpg filter=lfs diff=lfs merge=lfs -text     # JPEG images
*.jpeg filter=lfs diff=lfs merge=lfs -text    # JPEG images
*.gif filter=lfs diff=lfs merge=lfs -text     # GIF images
*.tiff filter=lfs diff=lfs merge=lfs -text    # TIFF images
*.psd filter=lfs diff=lfs merge=lfs -text     # Photoshop files

# Vector Graphics
*.svg filter=lfs diff=lfs merge=lfs -text     # Scalable Vector Graphics
*.svgz filter=lfs diff=lfs merge=lfs -text    # Compressed SVG files
*.eps filter=lfs diff=lfs merge=lfs -text     # Encapsulated PostScript files

# Fonts
*.ttf filter=lfs diff=lfs merge=lfs -text     # TrueType Font files
*.otf filter=lfs diff=lfs merge=lfs -text     # OpenType Font files
*.woff filter=lfs diff=lfs merge=lfs -text    # Web Open Font Format
*.woff2 filter=lfs diff=lfs merge=lfs -text   # Web Open Font Format 2

# High-Resolution and Large Media Files
*.bmp filter=lfs diff=lfs merge=lfs -text     # Bitmap images
*.exr filter=lfs diff=lfs merge=lfs -text     # High-dynamic-range images
*.hdr filter=lfs diff=lfs merge=lfs -text     # HDR images

# General Rules
*.log -text                                   # Exclude log files from text processing
*.tmp -text                                   # Exclude temporary files
*.bak -text                                   # Exclude backup files
*.dat -text                                   # Exclude cache files
*.swp -text                                   # Exclude swap files
